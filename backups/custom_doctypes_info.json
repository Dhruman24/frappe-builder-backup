{
  "custom_doctypes": [
    {
      "name": "Vendor",
      "module": "Vendor Manager",
      "custom": 1,
      "autoname": "field:vendor_name",
      "naming_rule": "By fieldname",
      "fields": [
        {
          "fieldname": "vendor_name",
          "fieldtype": "Data",
          "label": "Vendor Name",
          "reqd": 1
        },
        {
          "fieldname": "type",
          "fieldtype": "Select",
          "label": "Type",
          "options": "Supplier\nPartner\nDistributor"
        },
        {
          "fieldname": "email",
          "fieldtype": "Data",
          "label": "Email"
        },
        {
          "fieldname": "phone",
          "fieldtype": "Data",
          "label": "Phone"
        },
        {
          "fieldname": "status",
          "fieldtype": "Select",
          "label": "Status",
          "options": "Active\nInactive",
          "default": "Active"
        },
        {
          "fieldname": "description",
          "fieldtype": "Text",
          "label": "Description"
        }
      ],
      "permissions": [
        {
          "role": "System Manager",
          "read": 1,
          "write": 1,
          "create": 1,
          "delete": 1
        }
      ]
    },
    {
      "name": "Waitlist",
      "module": "Vendor Manager",
      "custom": 1,
      "autoname": "naming_series:",
      "naming_rule": "By Naming Series",
      "fields": [
        {
          "fieldname": "naming_series",
          "fieldtype": "Select",
          "label": "Series",
          "options": "WL-.####",
          "default": "WL-.####",
          "reqd": 1
        },
        {
          "fieldname": "full_name",
          "fieldtype": "Data",
          "label": "Full Name",
          "reqd": 1
        },
        {
          "fieldname": "email",
          "fieldtype": "Data",
          "label": "Email",
          "reqd": 1
        },
        {
          "fieldname": "vendor_type",
          "fieldtype": "Link",
          "label": "Vendor Type",
          "options": "Vendor"
        },
        {
          "fieldname": "notes",
          "fieldtype": "Text",
          "label": "Notes"
        }
      ],
      "permissions": [
        {
          "role": "System Manager",
          "read": 1,
          "write": 1,
          "create": 1,
          "delete": 1
        }
      ]
    }
  ],
